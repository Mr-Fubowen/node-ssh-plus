{
    "files.exclude": {
        "**/node_modules": false,
        "**/dist": false
    },
    "search.exclude": {
        "**/node_modules": true,
        "**/dist": true
    },
    "editor.formatOnSave": true,
    "explorer.fileNesting.expand": false,
